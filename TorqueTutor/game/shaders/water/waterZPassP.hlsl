//-----------------------------------------------------------------------------
// Structures                                                                  
//-----------------------------------------------------------------------------
struct ConnectData
{
   float4 hpos            : POSITION;
};


struct Fragout
{
   float4 col : COLOR0;
};


//-----------------------------------------------------------------------------
// Main                                                                        
//-----------------------------------------------------------------------------
Fragout main( ConnectData IN )
{
   Fragout OUT;

   OUT.col.x = 0.0;
   OUT.col.y = 0.0;
   OUT.col.z = 0.0;
   OUT.col.w = 0.1;

   return OUT;
}

